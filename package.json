{
  "name": "typebus-cqrs",
  "version": "0.1.0",
  "description": "Simple, type-safe CQRS library with auto-registration and minimal boilerplate (typebus-cqrs)",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "ts-node-dev --respawn --transpile-only src/examples/basic-usage.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "example:basic": "ts-node src/examples/basic-usage.ts",
    "example:advanced": "ts-node src/examples/advanced-usage.ts",
    "test:imports": "ts-node src/test-imports.ts",
    "test:build": "ts-node src/build-test.ts"
  },
  "keywords": [
    "cqrs",
    "command-query-responsibility-segregation",
    "typescript",
    "auto-registration",
    "message-bus",
    "domain-driven-design",
    "ddd",
    "type-safe",
    "minimal",
    "simple"
  ],
  "author": "Bayramov TG",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barsbay/typebus.git"
  },
  "bugs": {
    "url": "https://github.com/barsbay/typebus/issues"
  },
  "homepage": "https://github.com/barsbay/typebus#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rollup": "^3.25.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.0",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tslib": "^2.6.0"
  }
}